void main(
	float2 uv: TEXCOORD0, 
	
	out float4 oColour	: COLOR,

	uniform sampler normalDepthTex: register(s0),	
	uniform sampler colorTex: register(s1),
	uniform sampler lightTex: register(s2),
	uniform sampler ssaoTex: register(s3)
	)
{
	float3 color = tex2D(colorTex, uv);
	float4 normal = tex2D(normalDepthTex, uv);

	oColour  = float4(lerp(color,normal.xyz,normal.w),1.0);
}
