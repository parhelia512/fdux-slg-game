material NotSkinned/CastShadow/FourTexture
{ 
	technique default
	{
		scheme Default
		pass
		{
            vertex_program_ref ColorOnlyVP/NotSkinned/FourTexture
            {
            }
            fragment_program_ref ColorOnlyFP/FourTexture
            {
            }
			texture_unit Diffuse
			{
				tex_coord_set 0
				texture Desert.dds
			}
			texture_unit Diffuse1
			{
				tex_coord_set 1
				texture GreenLand.dds
				colour_op alpha_blend
			}
			texture_unit Diffuse2
			{
				tex_coord_set 2 
				texture Swamp.dds 		
				colour_op alpha_blend
			}
			texture_unit Diffuse3
			{
				tex_coord_set 3
				texture Snow.dds
				colour_op alpha_blend
			}
		}
	}
	technique specular
	{
		scheme SpecularEmissive
		pass
		{
			emissive 0.0 0.0 0.0
			specular 0.0 0.0 0.0 10.0
			vertex_program_ref SpecularEmissiveVP/NotSkinned/NoTexture
			{
			}
			fragment_program_ref SpecularEmissiveFP/NoTexture
			{
			}
		}
	}
    technique
    {
		scheme WriteDepthMap
        pass 
        {
            vertex_program_ref DepthShadowmap/CasterVP
            {
            }
            fragment_program_ref DepthShadowmap/CasterFP
            {
            }
        }
    }
	technique
	{
		scheme NormalDepth
		pass 
        {
            vertex_program_ref NormalDepthVP/NotSkinned/MultiTexture
            {
            }
            fragment_program_ref NormalDepthFP/MultiTexture
            {
            }
        }
	}
}