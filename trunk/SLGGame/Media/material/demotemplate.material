material Skinned/CastShadow/OneTexture
{ 
	technique default
	{
		scheme Default
		pass
		{
			cull_hardware none
		    vertex_program_ref ColorOnlyVP/Skinned/OneTexture
            {
            }
            fragment_program_ref ColorOnlyFP/OneTexture
            {
            }
			texture_unit Diffuse
			{
				texture Saint.dds
			}
		}
	}
	technique specular
	{
		scheme SpecularEmissive
		pass 
        {
			cull_hardware none
			emissive 0.0 0.0 0.0
			specular 1.0 1.0 1.0 50.0
			vertex_program_ref SpecularEmissiveVP/Skinned/NoTexture
			{
			}
			fragment_program_ref SpecularEmissiveFP/NoTexture
			{
			}
		}
	}
    technique
    {
		scheme WriteDepthMap
        pass 
        {
			cull_hardware none
            vertex_program_ref DepthShadowmap/CasterWithSkinVP
            {
            }
            fragment_program_ref DepthShadowmap/CasterFP
            {
            }
        }
    }
	technique
	{
		scheme NormalDepth
		pass 
        {
			cull_hardware none
            vertex_program_ref NormalDepthVP/Skinned/OneTexture
            {
            }
            fragment_program_ref NormalDepthFP/OneTexture
            {
            }
			texture_unit Diffuse
			{
				texture Saint.dds
			}
        }
	}
}

