import DepthShadowmap/Caster from "Shadow.material"
material SceneObj : DepthShadowmap/Caster
{
	technique default
	{
		scheme Default
		pass
		{
			cull_hardware none
			cull_software none
			
			scene_blend  alpha_blend
			alpha_rejection greater_equal 128
			texture_unit
			{
				texture SceneObj.dds
			}
		}
	}
}